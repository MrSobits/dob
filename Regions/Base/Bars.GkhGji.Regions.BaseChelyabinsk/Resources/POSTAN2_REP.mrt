        <Parameters isList="true" count="0" />
        <SqlCommand>select distinct   c.name as "Юр лицо" ,doc1.document_date as "дата", doc1.document_number as "номер", zp.name as "Комиссия" , zp.address as "АдресКомиссии", dv.npd_name as "НарушенныеСтатьи",  
	per.birthdate as "датарождения", per.address_birth as "месторождения", per.address_live as "место жительства", c.juridical_address as "место работы", r.fias_place_address as "Адрес нарушения",  
	zp.oktmo as "октмо", zp.bik as "бик", zp.inn as "инн", zp.kbk as "кбк", zp.kpp as "кпп", zp.uin as "уин", 
	res.name as "Тип постановления", 

	case when dis.type_violator = 1 then  indiv.name 
     when dis.type_violator = 3 then  indiv.name 
     when dis.type_violator = 2 then  c.name
     when dis.type_violator = 4 then  co.name end "Нарушитель",

case when dis.type_violator = 1 then  indiv.birth_place 
     when dis.type_violator = 3 then  indiv.birth_place  
     when dis.type_violator = 4 then  co.birthplace end "Место рождения",

case when dis.type_violator = 1 then  indiv.actually_residence 
     when dis.type_violator = 3 then  indiv.actually_residence  
     when dis.type_violator = 4 then  co.birthplace end "Место проживания", 

case when dis.type_violator = 1 then  indiv.job 
     when dis.type_violator = 3 then  indiv.job  
     when dis.type_violator = 4 then  c.name end "Место работы",
      
dv.name as "Нарушение",t1.document_date as "Дата определения",
case when dd1.position ='Председатель'then dd1.fio  else '' end "Председатель",
zp.address as "АдресКомиссии",string_agg((dd1.position ||', '||dd1.fio), '; ')as "Состав Комиссии",co.order_date::time as "ВремяНарушения", sum(rp.amount) as "РазмерШтрафа", tran1.namber_transport as  "Гос номер" , t.DELIVERY_DATE as "Дата вручения"

from GJI_RESOLUTION t
left join gkh_contragent c on c.id = t.contragent_id
left join gkh_contragent_contact co on co.contragent_id = c.id
left join gkh_dict_position pos on pos.id = co.position_id
left join gkh_person_placework pl on pl.contragent_id = c.id
left join gkh_person per on per.id = pl.person_id
left join gji_inspection i on i.contragent_id = c.id
left join gji_inspection_violation iv on iv.inspection_id = i.id
left join gji_dict_violation dv on dv.id = iv.violation_id
left join gji_protocol r on r.contragent_id = c.id
join gji_document doc1 on doc1.id = t.id
left join gji_resolution_payfine rp on rp.resolution_id = t.id
left join gji_protocol_artlaw pa on pa.protocol_id = t.id
left join gji_dict_articlelaw at on at.id = pa.articlelaw_id
join gji_document doc on doc.id = r.id
join GJI_NSO_PROTOCOL pr on pr.id=r.id
 left join gji_document_inspector n on n.document_id = doc.id
 left join gkh_dict_inspector dd on dd.id = n.inspector_id
 left join gkh_dict_zonainsp_inspect zz on zz.inspector_id = dd.id
 left join gkh_dict_zonainsp zp on zp.id = zz.zonal_inspection_id
 left join gkh_dict_zonainsp_munic zmun on zmun.zonal_inspection_id = zp.id
 left join gkh_dict_municipality mun on mun.id = zmun.municipality_id
 left join gji_document_children dc on dc.children_id= r.id
 left join GJI_DICT_CONCEDERATION_RESULT res on res.id = t.consideration_result_id 
left join gkh_dict_inspector dd1 on dd1.id in (select inspector_id from gkh_dict_zonainsp_inspect where zonal_inspection_id = zz.zonal_inspection_id)
left join GJI_RESOLUTION_DEFINITION t1 on t1.resolution_id = t.id
left join gji_disposal dis on dis.id = doc1.id
left join gkh_individual_person indiv on indiv.id = dis.individual_person_id
left join gji_transport_disposal tran on tran.disposal_id = dis.id
left join gji_transport tran1 on tran1.id = tran.transport_id 


 group by  "дата","номер","Комиссия","АдресКомиссии","НарушенныеСтатьи","датарождения","месторождения","место жительства","место работы","Адрес нарушения",
 "октмо","бик","инн","кбк","кпп","уин","Тип постановления","Нарушитель","Место рождения","Место проживания","Место работы","Нарушение","Дата определения","Председатель","ВремяНарушения","Гос номер", "Юр лицо" ,"Дата вручения"
 
 --where doc.id::bigint =  :id
</SqlCommand>
      </ИсточникДанных1>
