            <Text14 Ref="30" type="Text" isKey="true">
              <AllowHtmlTags>True</AllowHtmlTags>
              <Brush>Transparent</Brush>
              <CanGrow>True</CanGrow>
              <ClientRectangle>0.4,7.4,18,3.2</ClientRectangle>
