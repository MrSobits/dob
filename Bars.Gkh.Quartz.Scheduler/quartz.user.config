# Properties file for use by JobSchedulerFactory
# to create a Quartz Scheduler Instance
# http://quartz-scheduler.org/documentation/quartz-2.x/configuration/
# http://quartznet.sourceforge.net/tutorial/index.html

quartz.scheduler.batchTriggerAcquisitionMaxCount = 15

quartz.threadPool.type = Quartz.Simpl.SimpleThreadPool, Quartz
quartz.threadPool.threadCount = 25
quartz.threadPool.threadPriority = Normal

quartz.plugin.loggingTriggerHistoryPlugin.type = Bars.Gkh.Quartz.Scheduler.Listeners.LoggingTriggerHistoryPlugin, Bars.Gkh.Quartz.Scheduler
quartz.plugin.loggingJobHistoryPlugin.type = Bars.Gkh.Quartz.Scheduler.Listeners.LoggingJobHistoryPlugin, Bars.Gkh.Quartz.Scheduler