              <AllowHtmlTags>True</AllowHtmlTags>
              <Border>None;[105:105:105];1;Solid;False;4;[105:105:105]</Border>
              <Brush>Transparent</Brush>
              <ClientRectangle>0.4,9.4,3.6,0.6</ClientRectangle>
              <Conditions isList="true" count="0" />
